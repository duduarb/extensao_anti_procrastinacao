{
  "manifest_version": 3,
  "name": "Focar mais nos estudos",
  "version": "1.0",
  "description": "Uma extens√£o para me ajudar a focar nos estudos bloqueando sites que me distraem.",
  "permissions": ["storage", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}